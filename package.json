{
  "name": "megahack-brail2020",
  "version": "1.0.0",
  "description": "Mega Hackaton Brasil 2020",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage --verbose --runInBand --detectOpenHandles",
    "up:stubs": "json-server jsonserver/db.json --port 9999",
    "start:dev": "nodemon --exec babel-node -- src/index.js",
    "start": "babel-node -- src/index.js",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel src --out-dir lib",
    "clean": "rm -r ./build && mkdir ./build",
    "test:debugger": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "nodejs"
  ],
  "engines": {
    "node": "12.x"
  },
  "author": "Ju Dantas",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "jest": "^25.1.0",
    "sinon": "^8.1.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.1",
    "nodemon": "^2.0.2"
  }
}
